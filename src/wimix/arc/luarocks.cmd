@ECHO OFF
GOTO HANDLE

:APPEND_PATH
FOR %%# IN (%~n0%1.bat) DO IF NOT "%%~$PATH:#" == "" GOTO END
PUSHD .
CD /D "%~dp0\rocks\%1"
SET "Path=%Path%;%CD%"
POPD
GOTO END

:HANDLE
IF NOT EXIST "%~dp0\rocks\%1\%~n0%1.bat" GOTO USEDEFAULT
CALL :APPEND_PATH %1
%~n0%*
GOTO END

:USEDEFAULT
CALL :APPEND_PATH %1
%~n053 %*

:END
